      </tr>
    </table>

    <br/>
<div id="nav">
    <li><%= link_to "Apply for this Job", new_jobapplication_path, :method => :get %></li>
<% end%>

<li><%= link_to "Back To Home Page", session[:userHP], :method => :get %></li>
</div>
</div>
</body>
</html>
